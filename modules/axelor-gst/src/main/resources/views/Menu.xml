	<menuitem name="menu-party-root" title="Party"/>

	  	<menuitem name="menu-party-root-all" parent="menu-party-root" title="Party" action="action.party.all"/>
	  	<action-view name="action.party.all" title="Party" model="com.axelor.gst.db.Party">
	    	<view type="grid" name="party-grid"/>
	    	<view type="form" name="party-form"/>
	   	</action-view>
	  
	  	<menuitem name="menu-party-root-contact" parent="menu-party-root" title="Contact" action="action.contact.all"/>
	  	<action-view name="action.contact.all" title="Contact" model="com.axelor.gst.db.Contact">
	    	<view type="grid" name="contact-grid"/>
