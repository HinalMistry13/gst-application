	   	</action-view>
	   	
   		<menuitem name="menu-party-root-config" parent="menu-party-root" title="Configurations"/>
  	
		  	<menuitem name="menu-party-root-config-address" parent="menu-party-root-config" title="Address" action="action.config.address"/>
		  	<action-view name="action.config.address" title="Address" model="com.axelor.gst.db.Address">
		    	<view type="grid" name="address-grid"/>
		    	<view type="form" name="address-form"/>
		   	</action-view>
		   	
		   	<menuitem name="menu-party-root-config-city" parent="menu-party-root-config" title="City" action="action.config.city"/>
		  	<action-view name="action.config.city" title="City" model="com.axelor.gst.db.City">
		    	<view type="grid" name="city-grid"/>
		    	<view type="form" name="city-form"/>
		   	</action-view>
		   	
		   	<menuitem name="menu-party-root-config-state" parent="menu-party-root-config" title="State" action="action.config.state"/>
		  	<action-view name="action.config.state" title="State" model="com.axelor.gst.db.State">
		    	<view type="grid" name="state-grid"/>
		    	<view type="form" name="state-form"/>
		   	</action-view>
		   	
		   	<menuitem name="menu-party-root-config-country" parent="menu-party-root-config" title="Country" action="action.config.country"/>
		  	<action-view name="action.config.country" title="Country" model="com.axelor.gst.db.Country">
		    	<view type="grid" name="country-grid"/>
		    	<view type="form" name="country-form"/>
		   	</action-view>
		   	
  	<menuitem name="menu-product-root" title="Product"/>

	  	<menuitem name="menu-product-root-all" parent="menu-product-root" title="Product" action="action.product.all"/>
	  	<action-view name="action.product.all" title="Product" model="com.axelor.gst.db.Product">
	    	<view type="grid" name="product-grid"/>
	    	<view type="form" name="product-form"/>
	   	</action-view>
   	
   		<menuitem name="menu-product-root-category" parent="menu-product-root" title="Product Category" action="action.product.category"/>
	  	<action-view name="action.product.category" title="Product Category" model="com.axelor.gst.db.ProductCategory">
	    	<view type="grid" name="product-category-grid"/>
	    	<view type="form" name="product-category-form"/>
	   	</action-view>
   	
   	<menuitem name="menu-invoice-root" title="Invoice"/>
   	
   		<menuitem name="menu-invoice-root-create" parent="menu-invoice-root" title="Create Invoice" action="action.invoice.create"/>
	  	<action-view name="action.invoice.create" title="Invoice" model="com.axelor.gst.db.Invoice">
	    	<view type="form" name="invoice-form"/>
	    	<view type="grid" name="invoice-grid"/>
	   	</action-view>
	   	
	   	<menuitem name="menu-invoice-root-draft" parent="menu-invoice-root" title="Draft Invoices" action="action.invoice.draft"/>
	  	<action-view name="action.invoice.draft" title="Draft Invoices" model="com.axelor.gst.db.Invoice">
	    	<view type="grid" name="invoice-grid" />
			<domain>self.status = :status</domain>
			<context name="status" expr="draft"/>
		</action-view>
	   	
   		<menuitem name="menu-invoice-root-validated" parent="menu-invoice-root" title="Validated Invoices" action="action.invoice.validated"/>
	  	<action-view name="action.invoice.validated" title="Validated Invoices" model="com.axelor.gst.db.Invoice">
	    	<view type="grid" name="invoice-grid" />
			<domain>self.status = :status</domain>
			<context name="status" expr="validated"/>
		</action-view>
	   	
	   	<menuitem name="menu-invoice-root-paid" parent="menu-invoice-root" title="Paid Invoices" action="action.invoice.paid"/>
	  	<action-view name="action.invoice.paid" title="Paid Invoices" model="com.axelor.gst.db.Invoice">
	    	<view type="grid" name="invoice-grid" />
			<domain>self.status = :status</domain>
			<context name="status" expr="paid"/>
		</action-view>
	   	
	   	<menuitem name="menu-invoice-root-canceled" parent="menu-invoice-root" title="Canceled Invoices" action="action.invoice.canceled"/>
	  	<action-view name="action.invoice.canceled" title="Canceled Invoices" model="com.axelor.gst.db.Invoice">
	    	<view type="grid" name="invoice-grid" />
			<domain>self.status = :status</domain>
			<context name="status" expr="canceled"/>
		</action-view>
	   	
	   	<menuitem name="menu-invoice-root-all" parent="menu-invoice-root" title="All Invoices" action="action.invoice.all"/>
	  	<action-view name="action.invoice.all" title="All Invoices" model="com.axelor.gst.db.Invoice">
	    	<view type="grid" name="invoice-grid" />
	    	<view type="form" name="invoice-form"/>
		</action-view>
	   	
